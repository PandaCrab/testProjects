    "react-bootstrap": "^2.1.2",
    "react-chartjs-2": "^4.1.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-input-mask": "^3.0.0-alpha.2",
    "react-payment-inputs": "^1.1.9",
    "react-phone-input-2": "^2.15.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "react-scripts": "^5.0.1",
