{
    "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "declaration": false,
      "module": "esnext",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "importHelpers": true,
      "lib": ["es2015", "dom"],
      "target": "es5",
      "typeRoots": [
        "node_modules/@types"
      ],
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "allowJs": true,
      "moduleResolution": "node"
    }
  }